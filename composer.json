{
    "name": "codewithdennis/filament-translate-field",
    "description": "A plugin that lets you translate input of any field to another language.",
    "keywords": [
        "CodeWithDennis",
        "laravel",
        "filament-translate-field"
    ],
    "homepage": "https://github.com/codewithdennis/filament-translate-field",
    "support": {
        "issues": "https://github.com/codewithdennis/filament-translate-field/issues",
        "source": "https://github.com/codewithdennis/filament-translate-field"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "CodeWithDennis",
            "email": "personal.elsinga@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "CodeWithDennis\\FilamentTranslateField\\": "src/",
            "CodeWithDennis\\FilamentTranslateField\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeWithDennis\\FilamentTranslateField\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeWithDennis\\FilamentTranslateField\\FilamentTranslateFieldServiceProvider"
            ],
            "aliases": {
                "FilamentTranslateField": "CodeWithDennis\\FilamentTranslateField\\Facades\\FilamentTranslateField"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}