{
    "name": "codewithdennis/filament-translate-field",
    "description": "This package allows you to quickly translate filamentphp form field values, like converting a description from German to English, with just one click. It's an efficient solution for instantly translating content in your forms.",
    "keywords": [
        "CodeWithDennis",
        "laravel",
        "filament-translate-field",
        "filamentphp",
        "forms",
        "translate",
        "google-translate",
        "filament",
        "php",
        "forms",
        "plugin",
        "plugins",
        "package"
    ],
    "homepage": "https://github.com/codewithdennis/filament-translate-field",
    "support": {
        "issues": "https://github.com/codewithdennis/filament-translate-field/issues",
        "source": "https://github.com/codewithdennis/filament-translate-field"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "CodeWithDennis",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "stichoza/google-translate-php": "^5.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "CodeWithDennis\\FilamentTranslateField\\": "src/",
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeWithDennis\\FilamentTranslateField\\FilamentTranslateFieldServiceProvider"
            ],
            "aliases": {
                "FilamentTranslateField": "CodeWithDennis\\FilamentTranslateField\\Facades\\FilamentTranslateField"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
